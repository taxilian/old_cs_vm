class Factorial {
    int last;

    public int factorial(int i) {
        if (i == 1)
            return 1;
        else
            return factorial(i-1)*i;
    }
}
class Test {
    Factorial n;
    public void setFactorial(Factorial a) {
        n = a;
    }

    public int go(int i) {
        return n.factorial(i);
    }
}
void main( )
{
    Factorial n;
    Test t = new Test();
    n = new Factorial();
    t.setFactorial(n);
    cout << 'F';
    cout << ':';
    cout << ' ';
    cout << t.go(8);
    cout << '\n';
}

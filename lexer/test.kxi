class Message {
    private char msg[];
    int i;
    int end;

    public Message() {
        msg = new char[100];
        msg[0] = 'A';
        msg[1] = 'd';
        msg[2] = 'd';
        msg[3] = 'e';
        msg[4] = 'd';
        msg[5] = ' ';
        msg[6] = 'E';
        msg[7] = 'l';
        msg[8] = 'e';
        msg[9] = 'm';
        msg[10] = 'e';
        msg[11] = 'n';
        msg[12] = 't';
        msg[13] = ':';
        i = 14;
        msg[i] = 'D';
        msg[i+1] = 'u';
        msg[i+2] = 'p';
        msg[i+3] = 'l';
        msg[i+4] = 'i';
        msg[i+5] = 'c';
        msg[i+6] = 'a';
        msg[i+7] = 't';
        msg[i+8] = 'e';
        msg[i+9] = 'm';

        msg[24] = 'E';
        msg[25] = 'n';
        msg[26] = 't';
        msg[27] = 'e';
        msg[28] = 'r';

    }

    public void print(int i, int end) {
        while (i <= end) {
            cout << msg[i];
            i = i + 1;
        }
    }	
    public void printNum(int n) {
        cout << 'N';
        cout << 'u';
        cout << 'm';
        cout << ':';
        cout << ' ';
        cout << n;
        cout << '\n';
    }
    public void test() {
        i = 5;
        print(i,i);
    }
}
void main( )
{
    int i;
    char msg[] = new char[20];
    Message m = new Message();
    m.print(24, 28);
    i = 5;
    m.print(i,i);
    m.print(6,13);
    cin >> i;
    cout << 6 + i * 1000 - 3;
    m.printNum(6 + i * 1000 - 3);
    cout << '\n';
}
